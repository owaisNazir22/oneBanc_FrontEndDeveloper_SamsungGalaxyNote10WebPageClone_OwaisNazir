                //"x-ecom-jwt"			: "",
                //"x-ecom-web-jwt"		: "",
                "reporting"				: reporting,
                "originating_page"		: originating_page
            },

            headerParams_vals = {
            	"isHeaderRequired"	: "true",
            	"isCloseREquired"	: "true"
			};

        pageIns.initParams = initParams_vals;
        pageIns.user = user_vals;
        pageIns.headerParams = headerParams_vals;

        frameOnload(getFrameContainerObject, pageIns);
    }

    function setFrame(src) { // maybe.. not used 
        document.getElementById('chatBotIframe').setAttribute('src', src);
    }

    function getFrame(containerObject) {
        getFrameContainerObject = containerObject;
        document.getElementById("chatdiv").style.display="block";
        if(sessionStorage.getItem('cbSession')=='open'){ /*페이지 로딩에 챗창 열린 상태 확인*/
        	if (readCookie("cbEvents") == "minimized") {
	                document.getElementById("chatIfrmaeDiv").style.display="none";
	                document.getElementById("chatmindot").style.display="block";
	                document.getElementById("chatdiv").style.display="block";
	                if(chatbot_site.toLowerCase()=="in" && (readCookie('cbUnreadMsg') > 0) ) {
	                	//console.log("use unReadMsg cookie !!");
	            		$(".s-in-chat-number > *").next().text(readCookie('cbUnreadMsg'));
	            		$(".s-in-chat-number").css('display', 'block');
	                }
			} else {
            	showandhideiframe();
            }
        }
    }
    
    function openPopup() {
    	if ((!readCookie("cbEvents") || readCookie("cbEvents") == "closed") && document.getElementById("bot-cont").style.display=="none") { //챗이 닫혀있고 팝업없을때
        	//console.log("run openpopup");
            document.getElementById("chatimage").style.display="none";        
            document.getElementById("bot-cont").style.display = "block";    	    
        	
            if (typeof _satellite.getVar('chatPopupMsg') != 'undefined') {//popup msg
            	$(".bot-txt").text(_satellite.getVar('chatPopupMsg'));
            } else{
            	$(".bot-txt").text("Welcome to Samsung.com!! How can we help?");
            }            
    	}    	
    }
    
    window.onload = function(){
    	//console.log("window.onload");
    	var chatiFrameTag = '<iframe src="'+chatbot_chatclient+'" allowtransparency="true" id="chatBotIframe" allowtransparency="true" id="chatBotIframe" allowfullscreen="allowfullscreen" mozallowfullscreen="mozallowfullscreen" msallowfullscreen="msallowfullscreen" oallowfullscreen="oallowfullscreen" webkitallowfullscreen="webkitallowfullscreen" onload="getFrame(this)"></iframe>';
    	$("#chatIfrmaeDiv").html(chatiFrameTag);
    	    	
    	if(chatbot_site.toLowerCase()=="in"){ //popup click event
            $(".options.opt-1").click(function () { 
            	showandhideiframe();
            	fcTrack('click', 'floating chat');
                document.getElementById("bot-cont").style.display="none"; 	    
            	return false;            	
            });
            $(".options.opt-2").click(function () { 
                document.getElementById("chatimage").style.display = "block";        
                document.getElementById("bot-cont").style.display="none"; 	            	
            	return false;
            });
            $("#bot-cont > .web-widget-close").click(function () {
                document.getElementById("chatimage").style.display = "block";        
                document.getElementById("bot-cont").style.display="none"; 	            	
            	return false;
            });            
            
    		if (window.location.href.indexOf("/store/corporate") > -1 ) {
            	setTimeout(function(){
            		openPopup(); 
            		}, 120000);    	 			
    		} else {
            	setTimeout(function(){
            		openPopup(); 
            		}, 60000);    	    			
    		}	
    	} 	    	
    }
    
    

</script>
